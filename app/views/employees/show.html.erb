<h1>Employee List</h1>
<table>
  <tr>
    <th>ID</th>
    <th>F_Name</th>
    <th>L_Name</th>
    <th>DOB</th>
    <th>Email</th>
    <th>Address</th>
    <th>Contact_num.</th>
    <th>Designation</th>
    <th>Hire_date</th>
    <th>Resign_date</th>
    <th>Salary</th>
    <th>CTC</th>
    <th>Bank_Account</th>
      <th>EDIT</th>
    <th>DELETE</th>
  </tr>

  <tr>
    <td><%= @employee.id%></td>
    <td><%= @employee.f_name%></td>
    <td><%= @employee.l_name%></td>
    <td><%= @employee.dob%></td>
    <td><%= @employee.email%></td>
    <td><%= @employee.address%></td>
